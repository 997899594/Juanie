{
  "name": "@juanie/ui",
  "version": "0.1.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./tokens": {
      "import": "./dist/src/tokens/bilibili.js",
      "types": "./dist/src/tokens/bilibili.d.ts"
    },
    "./themes": {
      "import": "./dist/src/themes/bilibili.js",
      "types": "./dist/src/themes/bilibili.d.ts"
    },
    "./naive-ui": {
      "import": "./dist/src/naive-ui.js",
      "types": "./dist/src/naive-ui.d.ts"
    },
    "./theme": {
      "import": "./dist/src/theme.js",
      "types": "./dist/src/theme.d.ts"
    },
    "./types": {
      "import": "./dist/src/tokens/types.js",
      "types": "./dist/src/tokens/types.d.ts"
    },
    "./uno/bilibili": "./uno/bilibili.ts",
    "./uno/utils": "./uno/utils.ts",
    "./uno": "./uno/index.ts",
    "./css": {
      "import": "./dist/index.css"
    },
    "./styles/*": {
      "import": "./dist/styles/*"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rolldown && bunx tsc -b",
    "dev": "rolldown --watch",
    "type-check": "bunx tsc -b --noEmit"
  },
  "dependencies": {
    "naive-ui": "catalog:",
    "vue": "catalog:"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "catalog:",
    "rolldown": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "@unocss/preset-attributify": "catalog:",
    "@unocss/preset-icons": "catalog:",
    "@unocss/preset-uno": "catalog:",
    "unocss": "catalog:"
  },
  "packageManager": "bun@1.2.23",
  "engines": {
    "node": ">=22.0.0",
    "bun": ">=1.0.0"
  },
  "sideEffects": [
    "dist/index.css",
    "dist/styles/*.css"
  ]
}
