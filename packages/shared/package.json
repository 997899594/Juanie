{
  "name": "@juanie/shared",
  "version": "0.1.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./locale": {
      "import": "./dist/locale/index.js",
      "types": "./dist/locale/index.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "types": "./dist/server.d.ts"
    },
    "./ai-diagnostic": {
      "import": "./dist/src/ai-diagnostic.js",
      "types": "./dist/src/ai-diagnostic.d.ts"
    },
    "./cost-predict": {
      "import": "./dist/src/cost-predict.js",
      "types": "./dist/src/cost-predict.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "type-check": "tsc -b --noEmit"
  },
  "dependencies": {
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "vite": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^3.1.0"
  },
  "packageManager": "bun@1.2.23",
  "engines": {
    "node": ">=22.0.0",
    "bun": ">=1.0.0"
  }
}
