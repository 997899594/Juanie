  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./locale": {
      "import": "./dist/locale/index.js",
      "types": "./dist/locale/index.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "types": "./dist/server.d.ts"
    },
    "./ai-diagnostic": {
      "import": "./dist/src/ai-diagnostic.js",
      "types": "./dist/src/ai-diagnostic.d.ts"
    },
    "./cost-predict": {
      "import": "./dist/src/cost-predict.js",
