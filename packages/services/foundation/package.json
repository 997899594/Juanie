  "dependencies": {
    "@gitbeaker/rest": "^43.8.0",
    "@juanie/core": "workspace:*",
    "@juanie/types": "workspace:*",
    "@octokit/rest": "^22.0.1",
    "arctic": "^2.3.2",
    "libsodium-wrappers": "^0.7.15",
    "minio": "^8.0.2"
  },
  "devDependencies": {
    "@juanie/config-typescript": "workspace:*",
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/node": "^24.7.0",
    "typescript": "^5.9.3"
  }
}
