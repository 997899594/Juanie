{
  "name": "@juanie/service-foundation",
  "version": "0.1.0",
  "description": "Foundation services - Auth, Users, Organizations, Teams, Storage",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
