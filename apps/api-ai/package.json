{
  "name": "@juanie/api-ai",
  "version": "0.1.0",
  "description": "AI-Native DevOps Platform API - 2025 Bleeding Edge Edition",
  "type": "module",
  "main": "dist/main.js",
  "packageManager": "bun@1.1.38",
  "scripts": {
    "build": "bun run build:tsc",
    "build:tsc": "bun tsc -p tsconfig.build.json",
    "build:nest": "nest build",
    "dev": "bun --hot src/main.ts",
    "start": "bun run dist/main.js",
    "start:debug": "bun --inspect --hot src/main.ts",
    "start:prod": "bun run dist/main.js",
    "type-check": "bun tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "db:generate": "bun drizzle-kit generate",
    "db:migrate": "bun drizzle-kit migrate",
    "db:push": "bun drizzle-kit push",
    "db:studio": "bun drizzle-kit studio",
    "db:seed": "bun run src/database/seed.ts",
    "docker:build": "docker build -t juanie-api-ai .",
    "docker:run": "docker run -p 3001:3001 juanie-api-ai",
    "k8s:deploy": "kubectl apply -f k8s/",
    "observability:start": "docker-compose -f docker-compose.observability.yml up -d"
  },
  "dependencies": {
